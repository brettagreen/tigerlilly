{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/example.json",
    "type": "object",
    "properties": {
      "articleTitle": {
        "type": "string",
        "minLength": 2,
        "maxLength": 300
      },
      "authorId": {
        "type": "integer",
        "minimum": 1,
        "maximum": 1000
      },
      "text": {
        "type": "string",
        "minLength": 1
      },
      "issueId": {
        "type": "integer",
        "minimum": 1,
        "maximum": 1000
      }

    },
    "additionalProperties": false,
    "required": [
      "articleTitle",
      "authorId",
      "text",
      "issueId"
    ]
  }
  